{
  "name": "stashback",
  "version": "1.1.0",
  "description": "Stashes callbacks for later execution",
  "main": "index.js",
  "scripts": {
    "test": "node --expose-gc tests/testsuite.js",
    "example": "( cd example; node app.js )",
    "istanbul": "node --expose-gc node_modules/.bin/istanbul cover --report html --report lcov tests/testsuite.js",
    "codeclimate": "npm run istanbul && source .codeclimate",
    "lint": "eslint .",
    "doc": "jsdoc2md index.js > api.md",
    "precommit": "npm run lint && npm test",
    "prepush": "npm run codeclimate"
  },
  "keywords": [
    "callback",
    "manager",
    "stash",
    "timeout",
    "once"
  ],
  "author": "Stephen Cresswell",
  "license": "ISC",
  "devDependencies": {
    "async": "^2.6.1",
    "chance": "^1.0.16",
    "eslint": "^5.6.1",
    "eslint-config-imperative": "^2.1.0",
    "eslint-plugin-imperative": "^2.1.0",
    "express": "^4.16.3",
    "husky": "^1.1.1",
    "istanbul": "^0.4.5",
    "jsdoc-to-markdown": "^4.0.1",
    "redtape": "^1.0.0",
    "tape": "^4.9.1",
    "uuid": "^3.3.2"
  },
  "dependencies": {
    "debug": "^4.0.1",
    "lodash": "^4.17.11"
  },
  "directories": {
    "example": "example",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guidesmiths/stashback.git"
  },
  "bugs": {
    "url": "https://github.com/guidesmiths/stashback/issues"
  },
  "homepage": "https://guidesmiths.github.io/stashback/"
}
